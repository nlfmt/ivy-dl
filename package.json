{
  "name": "ivy-dl",
  "version": "1.0.0",
  "description": "A Desktop app to download youtube videos",
  "main": "main.js",
  "build": {
    "appId": "ivy-dl",
    "win": {
      "target": [
        "nsis"
      ],
      "requestedExecutionLevel": "asInvoker"
    },
    "nsis": {
      "uninstallDisplayName": "Ivy DL",
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "extraFiles": [
      "ext"
    ]
  },
  "scripts": {
    "start": "electron .",
    "watch": "webpack --config webpack.common.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nlfmt/ivy-dl.git"
  },
  "keywords": [
    "youtube",
    "downloader",
    "app",
    "converter"
  ],
  "author": "Tom F. (https://github.com/nlfmt)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nlfmt/ivy-dl/issues"
  },
  "homepage": "https://github.com/nlfmt/ivy-dl#readme",
  "dependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "cross-fetch": "^3.1.4",
    "css-loader": "^6.5.1",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-flip-move": "^3.0.4",
    "style-loader": "^3.3.1",
    "tree-kill": "^1.2.2",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1"
  },
  "devDependencies": {
    "babel-loader": "^8.2.3",
    "electron": "^16.0.2",
    "electron-reload": "^2.0.0-alpha.1"
  }
}
